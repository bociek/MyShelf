{% extends  'base.html.twig' %}

{% block body %}

    <h1>{{ 'title.tags_view'|trans({'%id%' : tags.id, '%abc%' : 'def'}) }}</h1>

    {% if paginator.data is defined and paginator.data|length %}
        <div>
            {{ tags.title }}, {{ bookmark.url }}
        </div>
        <div>
            <a href="{{ url('tags_index') }}" title="{{ 'label.back_to_list'|trans }}">
                {{ 'label.back_to_list'|trans }}
            </a>
        </div>
    {% else %}
        <div>
            {{ 'error_not_found'|trans }}
        </div>
    {% endif %}
{% endblock body %}